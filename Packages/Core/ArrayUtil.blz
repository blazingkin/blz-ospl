
# Currently implemented via bubble sort, will make quicksort at some point
:sort(arr)
	len = arr.length()
	for i = 0; i < len; i++
		for j = 0; j < len - 1; j++
			if arr[j] > arr[j + 1]
				temp = arr[j]
				arr[j] = arr[j + 1]
				arr[j + 1] = temp
			end
		end
	end
	return arr
end

:reverse(arr)
	len = arr.length()
	for i = 0; i < len / 2; i++
		temp = arr[i]
		arr[i] = arr[len - i - 1]
		arr[len - i - 1] = temp
	end
	return arr
end

:contains?(arr, element)
	len = arr.length()
	for i = 0; i < len; i++
		if arr[i] == element
			return true
		end
	end
	return false
end

:concatenate(arr1, arr2)
	len1 = arr1.length()
	len2 = arr2.length()
	for i = 0; i < len2; i++
		arr1[len1+i] = arr2[i]
	end
	return arr1
end

:slice(arr, start, end)
	# Later: add check for start/end that are out of bounds
	arr1 = []
	for i = start; i < end; i++
		arr1[i-start] = arr[i]
	end
	return arr1
end

