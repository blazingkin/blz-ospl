
# Currently implemented via bubble sort, will make quicksort at some point
:sort(arr)
	len = array_length(arr)
	for i = 0; i < len; i++
		for j = 0; j < len - 1; j++
			if arr[j] > arr[j + 1]
				temp = arr[j]
				arr[j] = arr[j + 1]
				arr[j + 1] = temp
			end
		end
	end
	return arr
end

:array_length(arr)
	ALEN arr len	# Call the internal function
	return len
end