:range(start_or_end, end)
    if end == {blz.nil}
        arr = []
        for i = start_or_end - 1; i >= 0; i--
            arr[i] = i
        end
        arr
    else
        arr = []
        for i = end - 1; i >= start_or_end; i--
            arr[i - start_or_end] = i
        end
        arr
    end
end