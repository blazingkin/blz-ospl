:main
	print("Enter a number")
	a = number_input()
	starttime = {system.time.currenttimemillis}
	for i = 0; i < a; i = i + 1
		print(i)
	end
	time = {system.time.currenttimemillis} - starttime
	timeperelement = time / a
	print("Took " + time + " milliseconds (" + timeperelement + " milliseconds per iteration)")
end