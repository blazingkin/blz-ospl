:main
	len = 0
	numon = 2
	ECHO "How many primes do you want?"
	NIN count
	
	WHILE len < count
		flag = 0
		mod = 10
		FOR i = 0, i < numon, i = i + 1
			MOD numon primes[i] mod
			IF mod == 0
				flag = 1
				BREAK
			END
		END
		numon = numon + 1
		IF flag == 1
			CONTINUE
		END
		primes[len] = numon - 1
		len = len + 1
	END
	
	FOR i = 0, i < count, i = i + 1
		off = i + 1
		ECHO off "th prime: " primes[i]
	END
		

END