:main
len = 0
numon = 2
ECHO "How many primes do you want?"
NIN count

WHILE len < count
found = 0
POW len .5 sqrt
FOR i = 0, i < sqrt, i = i + 1
MOD numon primes[i] flag
IF flag == 0
found = 1


ENDLOOP

numon = numon + 1
IF found == 1
ENDLOOP

prime = numon - 1
primes[len] = prime
len = len + 1

ENDLOOP


FOR i = 0, i < count, i = i + 1
offset = i + 1
ECHO offset "th prime: " primes[i]

ENDLOOP


END