:main
    #Test 00 :unique
    print("Test 00 unique on sorted array")
    arr = [1,2,2,3,3,3,4,4,5,5,6]
    arr2 = arr.unique()
    print(arr2)                                   # unique array (removed duplicates)
    print(arr)                                    # original array (unchanged)
    print("--------------------------------")

    #Test 01 :unique                                       #does not work if the array is unsorted
    print("Test 01 unique on unsorted array")
    arr = [3,4,2,3,9,2,6,0,1,4,8,3,3,7,9]
    arr2 = arr.unique()
    print(arr2)
    print(arr)
    print("--------------------------------")
    
end